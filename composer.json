{
    "name": "railto/twig-asset-version-extension",
    "description": "A twig extension for working with versioned assets using a rev-manifest file",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Mark Railton",
        "email": "mark@markrailton.com"
    }],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Railto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2|^8",
        "ext-json": "*",
        "twig/twig": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.0.1"
    },
  "scripts": {
    "cs": [
      "phpcs"
    ],
    "csfix": [
      "phpcbf"
    ],
    "test": [
      "phpunit"
    ]
  }
}
