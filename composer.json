{
    "name": "railto/twig-asset-version-extension",
    "description": "A twig extension for working with versioned assets using a rev-manifest file",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Mark Railton",
        "email": "mark@markrailton.com"
    }],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Railto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0",
        "twig/twig": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "squizlabs/php_codesniffer": "^3.0.1"
    }
}
